<div class="jumbotron">
<h3>Assigned to me</h3>
<% @subtasks.each do |subtask|%>
    <div class="d-flex justify-content-between">
    <div class="p-2">
    <h6><strong><%=subtask.name%></strong></h6>
    <h6>Status :<%=subtask.status%></h6>
    </div>
    <div class="p-2"><%= link_to "See More", subtask_path(subtask) ,class:"btn btn-primary btn-sm"%></div>
</div>
<%end%>
</div>